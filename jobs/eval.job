#!/bin/bash

#SBATCH --job-name=simple        # Job name
#SBATCH --output=job.%j.out     # Name of output file (%j expands to jobId)
#SBATCH --cpus-per-task=8        # Schedule one core
#SBATCH --time=03:00:00          # Run time (hh:mm:ss) - run for one hour max
